{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Docusaurus Start",
      "program": "${workspaceFolder}/node_modules/@docusaurus/core/bin/docusaurus.js",
      "args": ["start"],
      "cwd": "${workspaceFolder}"
